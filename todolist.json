    }
  },
  "servers": [
    {
      "description": "Todolist API",
      "url": "http://{envitonment}.todolist.localhost:3000",
      "variables": {
        "envitonment": {
          "default": "dev",
          "enum": ["dev", "test", "prod"]
        }
      }
    }
  ],
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  },
  "components": {},
  "paths": {
    "/todolist": {
      "get": {
        "summary": "Get all todolists",
        "description": "Get all todolists by default",
        "parameters": [
          {
            "name": "include_done",
            "in": "query",
            "required": false,
            "description": "Is include done todolist",
            "schema": {
              "type": "boolean",
              "nullable": true,
              "default": false
            }
          },
          {
            "name": "name",
            "in": "query",
            "required": false,
            "description": "Filter todolist by name",
            "schema": {
              "type": "string",
              "nullable": true,
              "minLength": 1,
              "maxLength": 100
            }
          }
        ],
        "responses": {}
      },
      "post": {
        "summary": "Create a new todolist",
        "description": "Create a new todolist to database",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "minLength": 1,
                    "required": true
                  },
                  "priority": {
                    "type": "integer",
                    "format": "int32",
                    "required": true,
                    "default": 1
                  }
                }
              }
            }
          }
        },
        "responses": {}
      }
    },
    "/todolist/{todoListId}": {
      "put": {
        "summary": "Update a todolist",
        "description": "Update a todolist",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "minLength": 1,
                    "required": true
                  },
                  "priority": {
                    "type": "integer",
                    "format": "int32",
                    "required": true,
                    "default": 1
                  }
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "todolistId",
            "in": "path",
            "required": true,
            "description": "Todolist id for updated",
            "schema": {
              "type": "string",
              "minLength": 1,
              "maxLength": 100
            }
          }
        ],
        "responses": {}
      },
      "delete": {
        "summary": "Delete a todolist",
        "description": "Delete a todolist",
        "parameters": [
          {
            "name": "todolistId",
            "in": "path",
            "required": true,
            "description": "Todolist id for deleted",
            "schema": {
              "type": "string",
              "minLength": 1,
              "maxLength": 100
            }
          }
        ],
        "responses": {}
      }
    }
  }
}
